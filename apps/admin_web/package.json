{
  "name": "admin-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings=0 .",
    "generate:api": "openapi-typescript ../../docs/api/admin.yaml -o src/types/api-admin.generated.ts && openapi-typescript ../../docs/api/search.yaml -o src/types/api-search.generated.ts",
    "prebuild": "npm run generate:api",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.16",
    "currency-codes": "^2.2.0",
    "libphonenumber-js": "^1.12.36",
    "next": "^16.1.6",
    "openapi-fetch": "^0.15.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
